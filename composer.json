{
  "name": "northrook/cache",
  "description": "PHP Memoization and native PHP data storage.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-zend-opcache": "*",
    "northrook/core": "dev-main",
    "symfony/cache": "@stable",
    "symfony/filesystem": "@stable",
    "symfony/var-exporter": "@stable"
  },
  "require-dev": {
    "northrook/dev-env": "dev-main"
  }
}