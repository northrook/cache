{
  "name": "northrook/cache",
  "description": "PHP Memoization and native PHP data storage.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Cache\\": "src/"
    },
    "files": [
      "src/cache.php"
    ]
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-zend-opcache": "*"
  },
  "require-dev": {
    "symfony/cache": "@stable",
    "northrook/dev-env": "dev-main"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse"
  }
}
